<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
  <appSettings>
    <add key="xmlPath" value="./data.xml"/>
    <add key="savePath" value="./CSV/"/>
  </appSettings>
  <system.serviceModel>
    <client>
      <endpoint address="net.tcp://localhost:8000/IFileHandling"
                binding="netTcpBinging"
                contract="Common.IFileHandling"
                />
    </client>
  </system.serviceModel>
  
</configuration>